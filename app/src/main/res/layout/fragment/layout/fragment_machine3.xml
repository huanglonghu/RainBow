    <data>

        <variable
            name="presenter"
            type="com.example.rainbow.base.Presenter" />

        <variable
            name="data"
            type="com.example.rainbow.bean.MachineDetailResponse.DataBean" />

        <import type="android.view.View" />

        <variable
            name="position"
